<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <Authors>Yustus</Authors>
    <Company>Yustus</Company>
    <Product>iikoServiceHelper_v2</Product>
    <Version>2.2.0</Version>
    <AssemblyName>iikoServiceHelper_v$(Version)</AssemblyName>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>true</SelfContained>
    <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
  </PropertyGroup>

  <PropertyGroup Condition="'$(SelfContained)' == 'false'">
    <AssemblyName>iikoServiceHelper_Compact</AssemblyName>
  </PropertyGroup>

</Project>